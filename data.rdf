  <rdf:Description rdf:about="http://www.sciedu.ca/journal/index.php/air/article/view/3159">
    <j.0:creator>中村覚</j.0:creator>
    <j.0:creator>大和裕幸</j.0:creator>
    <j.0:creator>岡田伊策</j.0:creator>
    <j.0:creator>深田直人</j.0:creator>
